<% include partials/header %>

<div class="row" style="text-align: center">
  <% childData.forEach(function(item){ %> <% if(typeof item == "object"){ %>
  <div class="card" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-title"><%= item.company %> <%= item.model %></h5>
      <p class="card-text">
        Mileage: <%= item.iMileage %> KM/L
      </p>
      <a href="/edit_vehicle_form?id=<%= item.key %>" class="btn btn-primary"
        >Edit Details</a
      >
    </div>
  </div>
  <% }%> <% }) %>
</div>

<% include partials/footer %>
