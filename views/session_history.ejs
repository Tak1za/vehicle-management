<% include partials/header.ejs %>

    <div class="accordion" id="accordionExample">
        <% childData.forEach(function(item){ %>
            <div class="card">
                <div class="card-header" id="headingOne<%=item.key %>">
                    <h2 class="mb-0">
                        <button class="btn btn-link" type="button" data-toggle="collapse" data-target="#collapseOne<%=item.key %>" aria-expanded="false"
                            aria-controls="collapseOne<%=item.key %>">
                            <%= item.company %> <%= item.model %>
                         </button>
                    </h2>
                </div>

                <div id="collapseOne<%=item.key %>" class="collapse" aria-labelledby="headingOne<%=item.key %>" data-parent="#accordionExample">
                    <div class="card-body">
                        <div class="col">
                            <% childData.forEach(function(item.Session){ %>
                                <div class="card mx-auto" style="width: 18rem;">
                                    <div class="card-body">
                                        <h6 class="card-subtitle mb-2 text-muted">
                                            Start Time:
                                            <%= item.Session.StartTime %>
                                        </h6>
                                        <h6 class="card-subtitle mb-2 text-muted">
                                            End Time:
                                            <%= item.Session.endTime %>
                                        </h6>
                                        <h6 class="card-subtitle mb-2 text-muted">
                                            End Time:
                                            <%= item.Session.finalOdometer %>
                                        </h6>
                                        <h6 class="card-subtitle mb-2 text-muted">
                                            Mileage:
                                            <%= item.Session.mileage %>
                                        </h6>
                                        <h6 class="card-subtitle mb-2 text-muted">
                                            Initial Fuel:
                                            <%= item.Session.iFuel %>
                                        </h6>
                                        <h6 class="card-subtitle mb-2 text-muted">
                                            Final Fuel:
                                            <%= item.Session.fFuel %>
                                        </h6>
                                        <a href="/individualGraph?id=<%= item.key %>" class="card-link">View Details</a>
                                    </div>
                                </div>
                                <% }) %>
                        </div>
                    </div>
                </div>
                <% }) %>
            </div>

            <% include partials/footer.ejs %>